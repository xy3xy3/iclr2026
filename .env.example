# Copy to .env and adjust values

# OpenAI-compatible embedding config
OPENAI_API_KEY=
# If using a compatible gateway, set base URL, e.g. https://api.openai.com/v1
OPENAI_BASE_URL=
# Model and dimension must match
OPENAI_EMBED_MODEL=text-embedding-3-small
OPENAI_EMBED_DIM=1536
EMBED_ONLY_MISSING=1
EMBED_BATCH=64
EMBED_CONCURRENCY=2
EMBED_MAX_RETRIES=5
EMBED_BACKOFF_BASE=1.5
EMBED_TASK_DELAY_MS=0
EMBED_LOG_FILE=data/embed.log
EMBED_LOG_APPEND=1

# Database (compose uses these defaults; adjust if needed)
POSTGRES_DB=iclr2026
POSTGRES_USER=iclr
POSTGRES_PASSWORD=iclrpass
POSTGRES_HOST=pgvector
POSTGRES_PORT=5432
